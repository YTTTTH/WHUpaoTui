<view class="banner">
  <swiper indicator-dots="true" autoplay="true" class="banner_body">
    <swiper-item wx:for="{{banner}}" wx:key="indexs" class="banner_item">
      <image src="{{item}}" mode="widthFix"></image>
    </swiper-item>
  </swiper>
</view>

<view class="notice">
  <image src="../../images/notice.png"></image>
  <text>A Test</text>
</view>

<view class="container">
  <view class="body" wx:for="{{indexConfig}}" wx:key="index" bindtap="toDetail" data-url="{{item.url}}">
   <view class="circleBorder">
    <view class="icon">
     <image src="{{ item.icon }}"></image>
   </view>
   </view>
   <text class="text">{{ item.text }}</text>
  </view>
</view>