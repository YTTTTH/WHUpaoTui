<view class="info" bindtap="updateInfo">
    <image class="avatar" src="{{ hasUserInfo ? userInfo.avatarUrl : '../../images/touxiang.png' }}"></image>
    <block wx:if="{{ !hasUserInfo }}">
        <button open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber"  wx:if="{{ canIUseGetUserProfile }}" bindtap="getUserProfile">点我登录</button>
        <button wx:else open-type="getUserInfo" bindgetuserinfo="getUserInfo">点我登录</button>
    </block>
    <view class="name" wx:if="{{ hasUserInfo }}">
        <text>{{ userInfo.nickName }}</text>
        <text>{{ userInfo.gender === 0 ? '男' : '女' }}</text>
    </view>
    <image wx:if="{{ hasUserInfo }}" class="into" src="../../images/dayu.png"></image>
</view>

<button class="cell mt" bindtap="applyOrder">
    <view class="cellLeft">
        <image src="../../images/apply.png"></image>
        <text>申请成为接单员</text>
    </view>
    <text class="cellRight">></text>
</button>

<button class="cell mt" bindtap="orderReceiver" wx:if="{{ admin }}">
    <view class="cellLeft">
        <image src="../../images/check.png"></image>
        <text>审核接单员申请</text>
    </view>
    <text class="cellRight">></text>
</button>

<view class='total' wx:if="{{!!userInfo&&personReceiveState==='success'}}">
  <view class='totalItem'>
    <text>我的接单数：</text>
    <text>{{helpTotalNum}}</text>
  </view>

  <view class='totalItem'>
    <text>我的接单收益：</text>
    <text>{{helpTotalMoney}}</text>
  </view>
  
</view>

